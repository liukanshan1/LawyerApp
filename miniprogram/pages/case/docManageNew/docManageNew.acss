.page {
  width: 100vw;
}
.top {
  display: flex;
  justify-content: space-between;
  width: 90vw;
  padding: 2vw 5vw;
}
.case {
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 80px;
  line-height: 80px;
  background-color: #fff;
  font-weight: bold;
  padding: 0 20px;
  border-bottom: 1px solid rgb(181, 180, 180);
}