.top_line{
  position: relative;
  background-color: white;
  height: 80px;
  width: 100%;
  box-sizing: border-box;
  align-items: center;
}

.title {
  width: 100%;
  text-align: center;
  padding-top: 20px;
}

.icons {
  position: absolute;
  right: 0;
  bottom: 0;
  margin-right: 5vw;
  z-index: 10;
}
.message{
  width: 6vw;
  height: 6vw !important;
  z-index: 10;
}
.ellipse {
  position: absolute;
  right: 0;
  top: 0;
  width: 2.5vw;
  height: 2.5vw !important;
}
